  "paths": {
    "/wechat/v1/base/check-qr-code": {
      "get": {
        "summary": "二维码结果",
        "operationId": "WeChat_CheckQRCode",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/baseCheckQRCodeResp"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
